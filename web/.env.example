# MCPeek Web Interface Configuration
# Copy this file to .env and customize for your environment

# Server Configuration
MCPEEK_HOST=127.0.0.1
MCPEEK_PORT=8080

# Logging Configuration
MCPEEK_LOG_LEVEL=INFO
MCPEEK_DEBUG=false

# CORS Configuration - Add your frontend URLs here
MCPEEK_CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:8080,http://127.0.0.1:8080

# Optional: Default API Keys (can be overridden in the interface)
# MCPEEK_API_KEY=your_default_api_key_here
# MCPEEK_AUTH_HEADER=Bearer your_token_here

# Optional: Default MCP Endpoint
# MCPEEK_DEFAULT_ENDPOINT=http://localhost:8000/mcp